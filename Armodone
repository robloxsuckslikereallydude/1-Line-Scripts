function Knifeswing()
attack = true
hum.WalkSpeed = 0
for i = 0,2,0.1 do
swait()
  	local Alpha = .3
       RootJoint.C0 = RootJoint.C0:lerp(cf(0.1,0 - 0.1 * math.cos(sine/15),-0.1)*angles(math.rad(-21),math.rad(-17.7),math.rad(-18))*RootCF,Alpha)
