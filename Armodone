Hilt3 = New("Part",Scythe,"Hilt3",{BrickColor = BrickColor.new("Dark orange"),Material = Enum.Material.SmoothPlastic,FormFactor = Enum.FormFactor.Custom,Size = Vector3.new(2.25000024, 0.639999986, 2.4000001),CFrame = CFrame.new(2.75282812, 13.5258093, -11.2364397, -3.15084421e-08, -1, 1.05408958e-07, -0.999999762, 3.15084208e-08, -1.49011612e-08, 1.49011612e-08, -1.05408979e-07, -0.999999762),Anchored = true,CanCollide = false,BackSurface = Enum.SurfaceType.SmoothNoOutlines,BottomSurface = Enum.SurfaceType.SmoothNoOutlines,FrontSurface = Enum.SurfaceType.SmoothNoOutlines,LeftSurface = Enum.SurfaceType.SmoothNoOutlines,RightSurface = Enum.SurfaceType.SmoothNoOutlines,TopSurface = Enum.SurfaceType.SmoothNoOutlines,Color = Color3.new(0.627451, 0.372549, 0.207843),})
Mesh = New("CylinderMesh",Hilt3,"Mesh",{})
