ParticleEmitter0.Parent = parent
ParticleEmitter0.Speed = NumberRange.new(0, 0)
ParticleEmitter0.Color = ColorSequence.new((brickcolor).Color,Color3.new(0,0,0))
ParticleEmitter0.Texture = "rbxassetid://459811994"
