function Weld2(part0,part1,c0,c1)
	local weld = Instance.new("Weld")
	weld.Parent = part0
	weld.Part0 = part0
	weld.Part1 = part1
	weld.C0 = c0 or cf()
	weld.C1 = c1 or cf()
	return weld
end

for _,v in pairs(NormalTime:GetDescendants()) do
	if v:IsA("Part") then
		v.Anchored = false
		v.Locked = true
		v.CanCollide = false
		Weld2(TimePieceHandle,v,TimePieceHandle.CFrame:inverse()*v.CFrame)
		v.Transparency = 1
	end
end

local AE = NewInstance('Weld',char,{Part0 = Torso, Part1 = TimePieceHandle, C0 = CF(0, 0, 0) * angles(Rad(0), Rad(0), Rad(0))})

New = function(Object, Parent, Name, Data)
	local Object = Instance.new(Object)
	for Index, Value in pairs(Data or {}) do
		Object[Index] = Value
	end
	Object.Parent = Parent
	Object.Name = Name
	return Object
end
