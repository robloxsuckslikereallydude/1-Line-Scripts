Mesh = New("CylinderMesh",ScHandle,"Mesh",{})


for _,v in pairs(Scythe:GetDescendants()) do
	if v:IsA("Part") then
		v.Anchored = false
		Weld2(ScHandle,v,ScHandle.CFrame:inverse()*v.CFrame)
		v.Locked = true
		
		v.CanCollide = false
		v.Transparency = 1
	end
end


