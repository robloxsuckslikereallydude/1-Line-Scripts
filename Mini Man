hatset('rol_icebrainAccessory','Torso',CFrame.new(),reanim['rol_icebrainAccessory'].Handle.AccessoryWeld.C0:Lerp(CF(0.2+0*math.cos(sine/13),1.5+0*math.cos(sine/13),1+0*math.cos(sine/13))*ANGLES(RAD(0+0*math.cos(sine/13)),RAD(-31+0*math.cos(sine/13)),RAD(0+0*math.cos(sine/13))),1),true)
