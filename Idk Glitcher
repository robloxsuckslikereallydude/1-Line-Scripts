	LWP2W.C0 = LWP2W.C0:lerp(CF.N(0,12,0)*CF.A(M.R(-90+1000*M.C(WingSine/38)),0,M.R(45)),.2)
	LWP3W.C0 = LWP3W.C0:lerp(CF.N(0,12,0)*CF.A(M.R(90+1000*M.C(WingSine/38)),0,M.R(-45)),.2)
	
	RWP1W.C0 = RWP1W.C0:lerp(CF.N(0,12,0)*CF.A(M.R(-90+1000*M.C(WingSine/38)),0,M.R(-45)),.2)
	RWP2W.C0 = RWP2W.C0:lerp(CF.N(0*M.C(WingSine/32),.5+0*M.C(WingSine/32),-1)*CF.A(0,0,M.R(0-1500*M.C(WingSine/50))),.2)
	RWP3W.C0 = RWP3W.C0:lerp(CF.N(0*M.C(WingSine/32),.5+0*M.C(WingSine/32),-1)*CF.A(0,0,M.R(120-1500*M.C(WingSine/50))),.2)
	RWP4W.C0 = RWP4W.C0:lerp(CF.N(0*M.C(WingSine/32),.5+0*M.C(WingSine/32),-1)*CF.A(0,0,M.R(-120-1500*M.C(WingSine/50))),.2)
	RWP5W.C0 = RWP5W.C0:lerp(CF.N(0*M.C(WingSine/32),1+1.5*M.C(WingSine/32),-3-2*M.C(WingSine/32))*CF.A(0,0,M.R(-120+1500*M.C(WingSine/50))),.2)
