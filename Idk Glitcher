	--custom walks--
	
	    elseif(State == 'Walk')then
		if(Mode=='RIDDLER' or Mode=='INTERGRITY')then
			local Alpha = .1
  			if(NeutralAnims)then
 				RJ.C0 = RJ.C0:lerp(RJC0*CF.N(0,4+2*M.C(Sine/52),0)*CF.A(M.R(-15+5*M.S(Sine/58)),M.R(0+5*M.C(Sine/42)),0),Alpha)
