plr.CharacterAdded:Connect(function(c)
	char:Destroy()
	local newCam = Instance.new("Camera",workspace)
	workspace.CurrentCamera = newCam
	newCam.CameraSubject = c:FindFirstChildOfClass("Humanoid")
	alive = false
end)
