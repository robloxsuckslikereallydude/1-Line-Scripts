	if not rotationvalue or not fix(rotationvalue,script) or rotationvalue.Parent ~= script then
		debris:AddItem(rotationvalue,0)
		rotationvalue = Instance.new("CFrameValue",script)
		rotationvalue.Name = randomstring()
	end
end)

