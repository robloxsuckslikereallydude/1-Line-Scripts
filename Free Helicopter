-- HATS NEEDED: https://pastebin.com/raw/rnsF0d60

local tangerex = loadstring(game:HttpGet("https://raw.githubusercontent.com/PYXDYT/TGX-/main/Template"))()

reanimate = tangerex:Reanimate("TF",true --[[Usage: true or false to toggle click fling]]) -- SP / Simple, LF / Leg Fling, TF / Torso Fling, PD / Perma Death [Fling]
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 30

game.Players.LocalPlayer.Character.Humanoid.JumpPower = 0

reanimate:Animation("idle",function()

reanimate:Hat("Pink Hair","Head",true,function()
workspace.Camera.CameraSubject.Parent["Pink Hair"].Handle.AccessoryWeld.C0=workspace.Camera.CameraSubject.Parent["Pink Hair"].Handle.AccessoryWeld.C0:Lerp(CFrame.new(3+0*math.cos(sine/10),1.5+0*math.cos(sine/10),0+0*math.cos(sine/10))*CFrame.Angles(math.rad(0+0*math.cos(sine/10)),math.rad(90+0*math.cos(sine/10)),math.rad(0+0*math.cos(sine/10))),0.5)
end)

reanimate:Hat("MessyHair","Head",true,function()
workspace.Camera.CameraSubject.Parent["MessyHair"].Handle.AccessoryWeld.C0=workspace.Camera.CameraSubject.Parent["MessyHair"].Handle.AccessoryWeld.C0:Lerp(CFrame.new(0+0*math.cos(sine/10),5.3+0*math.cos(sine/10),-1.5+0*math.cos(sine/10))*CFrame.Angles(math.rad(70+0*math.cos(sine/10)),math.rad(0+0*math.cos(sine/2)),math.rad(0+0*math.cos(sine/10))),0.5)
end)

reanimate:Hat("MessyHair","Head",true,function()
workspace.Camera.CameraSubject.Parent["MessyHair"].Handle.AccessoryWeld.C0=workspace.Camera.CameraSubject.Parent["MessyHair"].Handle.AccessoryWeld.C0:Lerp(CFrame.new(0+0*math.cos(sine/10),6+0*math.cos(sine/10),-3+0*math.cos(sine/10))*CFrame.Angles(math.rad(70+0*math.cos(sine/10)),math.rad(0+0*math.cos(sine/2)),math.rad(0+0*math.cos(sine/10))),0.5)
end)

reanimate:Hat("Kate Hair","Head",true,function()
