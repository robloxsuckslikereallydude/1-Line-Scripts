reanimate:Animation("jump",function()

game.Players.LocalPlayer.Character.Humanoid.JumpPower = 0

reanimate:Hat("Pink Hair","Head",true,function()
workspace.Camera.CameraSubject.Parent["Pink Hair"].Handle.AccessoryWeld.C0=workspace.Camera.CameraSubject.Parent["Pink Hair"].Handle.AccessoryWeld.C0:Lerp(CFrame.new(3+0*math.cos(sine/10),1.5+0*math.cos(sine/10),0+0*math.cos(sine/10))*CFrame.Angles(math.rad(0+0*math.cos(sine/10)),math.rad(90+0*math.cos(sine/10)),math.rad(0+0*math.cos(sine/10))),0.5)
end)

