function add()
 for i,v in pairs({la,ra,ll,rl,hed,trs,trs1,gun}) do
  if v[1] then
   --velocity(v[1])
   v[4] = align(v[1],Root)
   table.insert(aligners,{
    align = v[4],
    part = v[1]
   })
  end
 end
end
add()
